<h1>{{req.session.username.username}} MEMES</h1>

{{#if isuser}}
<form action="{{_id}}" method= "POST" enctype="multipart/form-data">
    {{!-- <label for="name">Name</label>
    <input type="text" name="name" id="name"/>

    <br> --}}

    <label for="imageUrl">Image
    <input type="file" name="imageUrl" />
    </label>

    <button type="submit">Save changes</button>
</form>

{{/if}}

<ul>
    {{#each user.memes}}
    <li>
        <img src="{{this.imageUrl}}" style="width: 300px">
        
    </li>
    {{/each}}
</ul>

